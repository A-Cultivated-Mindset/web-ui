{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "target": "es6",
        "jsx": "react",
        "module": "es6",
        "moduleResolution": "node",
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    },
    "exclude": ["node_modules"]
}
